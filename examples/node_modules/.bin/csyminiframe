#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chaoshengyang/Desktop/miniframe/packages/csyminiframe/bin/node_modules:/Users/chaoshengyang/Desktop/miniframe/packages/csyminiframe/node_modules:/Users/chaoshengyang/Desktop/miniframe/packages/node_modules:/Users/chaoshengyang/Desktop/miniframe/node_modules:/Users/chaoshengyang/Desktop/node_modules:/Users/chaoshengyang/node_modules:/Users/node_modules:/node_modules:/Users/chaoshengyang/Desktop/miniframe/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/chaoshengyang/Desktop/miniframe/packages/csyminiframe/bin/node_modules:/Users/chaoshengyang/Desktop/miniframe/packages/csyminiframe/node_modules:/Users/chaoshengyang/Desktop/miniframe/packages/node_modules:/Users/chaoshengyang/Desktop/miniframe/node_modules:/Users/chaoshengyang/Desktop/node_modules:/Users/chaoshengyang/node_modules:/Users/node_modules:/node_modules:/Users/chaoshengyang/Desktop/miniframe/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../csyminiframe/bin/csyminiframe.js" "$@"
else
  exec node  "$basedir/../csyminiframe/bin/csyminiframe.js" "$@"
fi
